@model List<MeyawoPortfolio.Models.TblEducation>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}


<table class="table table-bordered">

    <tr>
        <th> Education Id</th>
        <th> Okul İsim </th>
        <th> Bölümü </th>
        <th> SIL</th>
        <th> Guncelle </th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EducationId</td>
            <td>@item.OkulAdı</td>
            <td>@item.Bölüm</td>
            <td><a href="/Education/DeleteEducation/@item.EducationId" class="btn btn-danger"> Sil</a>
            <td><a href="/Education/UpdateEducation/@item.EducationId" class="btn btn-success"> Güncelle</a> </td>

        </tr>
    }

</table>

<td> <a href="/Education/CreateEducation/" class="btn btn-primary"> Yeni Eğitim Hayatı  Girişi</a></td>
